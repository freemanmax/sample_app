<header>
    <nav>
       <ul>
           <li><%= link_to "Home", root_path, title: "Home" %></li>
           <% if loged_in? %>
             <li><%= link_to "Users", users_path, title: "Users" %></li>
             <li><%= link_to "Profile", user_path(current_user), title: "Profile" %></li>
             <li><%= link_to "Settings", edit_user_path(current_user), title: "Settings" %></li>
             <li><%= link_to "Logout", logout_path, method: "delete", title: "Logout" %></li>
           <% else %>
             <li><%= link_to "Register", register_path, title: "Register" %></li>
             <li><%= link_to "Login", login_path, title: "Login" %></li>
           <% end %>
       </ul>
    </nav>
</header>
